  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "request": "launch",
      "name": "Github - Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/githunter-scraper.js",
      "args": [
        "--scraperPoint",
        "trending.github",
        "--nodes",
        "code"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Gitlab - Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/githunter-scraper.js",
      "args": [
        "--scraperPoint",
        "trending.gitlab",
        "--nodes",
        "code"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server - Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/githunter-scraper.js",
      "args": [
        "--server"
      ]
    },
  ]
}
